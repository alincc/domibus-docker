
functionsFile="${BASH_SOURCE[0]}"
echo "   - ${functionsFile}"

. ${SCRITPATH}/scripts/functions/initDatabaseMySQL
. ${SCRITPATH}/scripts/functions/initDatabaseOracle

function initDatabase {
   if [ "${DatabaseType}" == "MySQL" ] ; then
      echo "initialazing initDatabaseMySQL"
      initDatabaseMySQL
   fi
   if [ "${DatabaseType}" == "Oracle" ] ; then
      initDatabaseOracle
   fi
}

